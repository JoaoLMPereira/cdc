<?xml version="1.0" encoding="UTF-8"?>
<plugin title="Pentaho Distributed Cache" loader="DEFAULT">
  <lifecycle-listener class="pt.webdetails.cdc.CdcLifeCycleListener"/>
  <static-paths>
    <static-path url="/cdc/static" localFolder="static"/>
  </static-paths>
  
  <content-types />
    <content-generator scope="local" id="cdc" type="graphicsGenerator" url="">
      <classname>pt.webdetails.cdc.CdcContentGenerator</classname>
      <title>CDC</title>
    </content-generator>
  <!-- Menu entry -->
  <menu-items/>
  
  <webservice  
    id="MondrianCacheCleanService" 
    type="xml" 
    class="pt.webdetails.cdc.MondrianCacheCleanService"/>

  <webservice  
    id="ExtPluginConfigService" 
    type="xml" 
    class="pt.webdetails.cdc.ws.ExtPluginConfigService"/>
    
  <webservice  
    id="HazelcastMonitor" 
    type="xml" 
    class="pt.webdetails.cdc.ws.HazelcastMonitorService"/>
    
</plugin>
